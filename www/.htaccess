###
# SERVER CONFIGURATION
###

#SetEnv PHP_VER 5
SetEnv PHP_VER 5_TEST

# Server do following symlinks
 Options +FollowSymlinks
# don't show content dir
 Options -Indexes


###
# REDIRECTION
###

#Set Base of rewratting and redirection

#HANAKO redirect
RedirectMatch 301 index.html$ index.php

# Activating rewritting module :
RewriteEngine on

# Errors rewritting
ErrorDocument 503 error/error-503.html
ErrorDocument 400 error/error-400.html
ErrorDocument 401 error/error-401.html
ErrorDocument 403 error/error-403.html
ErrorDocument 500 error/error-500.html
ErrorDocument 404 error/error-404.html

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond $1 !^(index\.php)
RewriteRule ^(.*)$ index.php/$1 [L]